        stage('Test') { 
            steps {
                echo "In Test Stage" 
            }
        }
        stage('Deploy') { 
            steps {
                echo "In Deploy Stage" 
            }
        }
		stage('Archive') { 
            steps {
                echo "In Archive Stage" 
            }
        }
    }
}
